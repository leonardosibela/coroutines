<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sibela.coroutinescourse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoroutinesCourse">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".BasicExampleActivity"
            android:label="@string/first_example" />

        <activity
            android:name=".AlteringContextActivity"
            android:label="@string/second_example" />

        <activity
            android:name=".NetworkTimeoutActivity"
            android:label="@string/network_timeout" />

        <activity
            android:name=".JobActivity"
            android:label="@string/coroutines_jobs" />

        <activity
            android:name=".ParallelJobsActivity"
            android:label="@string/parallel_jobs" />

        <activity
            android:name=".AsyncAndAwaitActivity"
            android:label="@string/async_and_await" />

        <activity android:name=".SequentialJobWithAsyncAndAwaitActivity"
            android:label="@string/sequential_job_async_await"/>

    </application>

</manifest>